{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "required": false
    },
    "otp": {
      "type": "string"
    },
    "otpSentAt": {
      "type": "datetime"
    },
    "otpExpiredAt": {
      "type": "datetime"
    },
    "mobile": {
      "type": "string",
      "required": true
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    }
  }
}
