{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "required": false
    },
    "otp": {
      "type": "string",
      "private": true
    },
    "otpSentAt": {
      "type": "datetime",
      "private": true
    },
    "otpExpiredAt": {
      "type": "datetime",
      "private": true
    },
    "mobile": {
      "type": "string",
      "required": true,
      "regex": "^\\+989\\d{9}$"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::child.child",
      "mappedBy": "user"
    },
    "name": {
      "type": "string"
    },
    "rahches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rahche.rahche",
      "mappedBy": "user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user"
    },
    "lockPassword": {
      "type": "password",
      "private": true
    },
    "lockOption": {
      "type": "enumeration",
      "enum": [
        "question",
        "password"
      ],
      "default": "question",
      "required": true
    },
    "fixedLogin": {
      "type": "boolean"
    }
  }
}
